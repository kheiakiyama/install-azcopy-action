    "azcopy",
    "install",
    "setup"
  ],
  "author": "kheiakiyama",
  "license": "MIT",
  "dependencies": {
    "@actions/core": "^1.9.1",
    "@actions/io": "^1.0.2",
    "@actions/tool-cache": "^2.0.1",
    "actions-secret-parser": "^1.0.3",
    "underscore": "^1.12.1"
  },
  "devDependencies": {
    "@types/jest": "^26.0.24",
    "@types/node": "^14.14.19",
    "@typescript-eslint/parser": "^4.11.1",
    "@vercel/ncc": "^0.38.1",
    "eslint": "^7.17.0",
    "eslint-plugin-github": "^4.1.1",
    "eslint-plugin-jest": "^24.1.3",
    "js-yaml": "^3.14.1",
    "prettier": "^2.2.1",
    "ts-jest": "^29.1.1",
    "typescript": "^4.1.3"
  }
}
