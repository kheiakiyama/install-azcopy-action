{
  "name": "install-azcopy-action",
  "version": "0.0.0",
  "private": true,
  "description": "install azcopy action",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "format": "prettier --write **/*.ts",
    "format-check": "prettier --check **/*.ts",
    "lint": "eslint src/**/*.ts",
    "pack": "ncc build",
    "test": "jest",
    "all": "npm run build && npm run format && npm run lint && npm run pack && npm test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kheiakiyama/install-azcopy-action.git"
  },
  "keywords": [
